#!/usr/bin/env php
<?php

set_time_limit(0);

libxml_use_internal_errors(true);

date_default_timezone_set('Asia/Bishkek');

if (file_exists(__DIR__ . '/../vendor/autoload.php')) {
    require_once __DIR__ . '/../vendor/autoload.php';
} else {
    require_once __DIR__ . '/../../../../vendor/autoload.php';
}

(new DieselUp)->invoke();
